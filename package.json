{
  "name": "iconic-comments",
  "displayName": "Iconic Comments",
  "description": "Emoji-powered comment snippets for better code documentation and organization",
  "version": "1.0.1",
  "publisher": "srd",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "comments",
    "snippets",
    "emoji",
    "documentation",
    "code-organization",
    "typescript",
    "javascript",
    "tsx",
    "jsx"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescriptreact.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascriptreact.json"
      }
    ],
    "commands": [
      {
        "command": "iconic-comments.showCheatsheet",
        "title": "Show Iconic Comments Cheatsheet",
        "category": "Iconic Comments"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^2.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soumyaRauth/iconic-comments.git"
  },
  "bugs": {
    "url": "https://github.com/soumyaRauth/iconic-comments/issues"
  },
  "homepage": "https://github.com/soumyaRauth/iconic-comments#readme",
  "license": "MIT",
  "icon": "images/icon.png"
}
